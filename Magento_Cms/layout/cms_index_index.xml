<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.top">
            <block class="Magento\Framework\View\Element\Template" name="home-banner" before="-" template="Magento_Cms::home-banner.phtml" />
        </referenceContainer>
        <referenceContainer name="main">
            <block class="Magento\Framework\View\Element\Template" name="home-main-content" before="-" template="Magento_Cms::home-main-content.phtml" />
        </referenceContainer>
    </body>
</page>
